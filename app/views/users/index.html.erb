<div><%= flash[:notice] %></div>
<h1>Users</h1>
<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan = "3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= image_tag user.get_profile_image(100, 100) %></td>
      <td><%= user.name %></td>
      <td><%= link_to "Show",user_path(user.id) %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<%= render 'user',user: @user %>
<%= link_to "Edit", edit_user_path(@user.id) %>
<%= render 'books/book',book: @book %>